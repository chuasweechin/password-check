{
    "name": "Password Check",
    "version": "1.0.8",
    "description": "Password Check",
    "permissions": [
    	"activeTab",
    	"declarativeContent"
    ],
    "background": {
      "scripts": ["js/background.js"],
      "persistent": false
    },
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/lock-16.png",
        "32": "images/lock-32.png",
        "48": "images/lock-48.png",
        "128": "images/lock-128.png"
      }
    },
    "icons": {
      "16": "images/lock-16.png",
      "32": "images/lock-32.png",
      "48": "images/lock-48.png",
      "128": "images/lock-128.png"
    },
    "manifest_version": 2
}